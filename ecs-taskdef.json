{
  "family": "resume-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::977098999802:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "resume",
      "image": "977098999802.dkr.ecr.us-east-1.amazonaws.com/my-portfolio:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5001,
          "protocol": "tcp"
        }
      ]
    }
  ]
}

